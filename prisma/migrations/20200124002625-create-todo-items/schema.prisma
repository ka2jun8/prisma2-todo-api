generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model TodoItem {
  text    String
  todo_id Int    @id @default(autoincrement())
  user_id User
}

model User {
  user_id   Int        @id @default(autoincrement())
  name      String
  todoItems TodoItem[]
}

