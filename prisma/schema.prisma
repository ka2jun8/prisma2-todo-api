generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://prisma:prisma@localhost:5432/prisma?schema=public"
}

model todos {
  text    String
  todo_id Int    @id
  user_id users
}

model users {
  name    String
  user_id Int     @id
  todoses todos[]
}