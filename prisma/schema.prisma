generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://prisma:prisma@localhost:5432/prisma?schema=public"
}

model TodoItem {
  text    String
  todo_id Int    @id @default(cuid())
  user_id User
}

model User {
  name      String
  user_id   Int        @id @default(cuid())
  todoItems TodoItem[]
}
